    <title>Ville Ecologique</title>
</head>
<body>
    <button onclick="window.location.href = '/ville-eco';"> Page principale </button>
    <div class="container glass">
            <% data.forEach(element => { %>
            <h2><%= element.ville %></h2>
              
        <p><%= element.commentaire %></p>
          
            <% }) %>
          
            <form action="/commentaire" method="post">
                <input type="text" placeholder="ville" name="ville">
                <input type="text" placeholder="commentaire" name="commentaire">
                <button type="submit">Envoyer</button>
            </form>

    
</body>
